  </data>
  <data name="SettingsCommand" xml:space="preserve">
    <value>Open settings</value>
  </data>
  <data name="LanguageCommand" xml:space="preserve">
    <value>Change language</value>
  </data>
  <data name="LanguageChanged" xml:space="preserve">
    <value>Language has been changed to {0}</value>
  </data>
  <data name="InvalidCommand" xml:space="preserve">
    <value>Invalid command. Please try again.</value>
  </data>
  <data name="ErrorOccurred" xml:space="preserve">
    <value>An error occurred: {0}</value>
  </data>
  <data name="AvailableCommands" xml:space="preserve">
    <value>Available commands:</value>
  </data>
  <data name="LanguageButton" xml:space="preserve">
    <value>Language Settings</value>
  </data>
  <data name="ThemeButton" xml:space="preserve">
    <value>Theme Settings</value>
  </data>
  <data name="SettingsMessage" xml:space="preserve">
